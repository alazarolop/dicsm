  ids = {Melendez-Pastor2010ApplyingImagingSpectroscopya},
  title = {Applying Imaging Spectroscopy Techniques to Map Saline Soils with {{ASTER}} Images},
  author = {Melendez-Pastor, I. and Navarro-Pedreño, J. and Koch, M. and Gómez, I.},
  date = {2010-08},
