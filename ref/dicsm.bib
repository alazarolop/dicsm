@article{Orton2018QuantifyingEconomicImpact,
  title = {Quantifying the Economic Impact of Soil Constraints on {{Australian}} Agriculture: {{A}} Case-Study of Wheat},
  shorttitle = {Quantifying the Economic Impact of Soil Constraints on {{Australian}} Agriculture},
  author = {Orton, Thomas G. and Mallawaarachchi, Thilak and Pringle, Matthew J. and Menzies, Neal W. and Dalal, Ram C. and Kopittke, Peter M. and Searle, Ross and Hochman, Zvi and Dang, Yash P.},
  date = {2018-11},
