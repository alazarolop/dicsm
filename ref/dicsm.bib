@article{Temme2016LORICANewModel,
  title = {{{LORICA}} – {{A}} New Model for Linking Landscape and Soil Profile Evolution: {{Development}} and Sensitivity Analysis},
  shorttitle = {{{LORICA}} – {{A}} New Model for Linking Landscape and Soil Profile Evolution},
  author = {Temme, Arnaud J.A.M. and Vanwalleghem, Tom},
  date = {2016-05},
  journaltitle = {Comput. Geosci.},
  volume = {90},
