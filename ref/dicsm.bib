@article{Sharififar2019AddressingIssueDigital,
  title = {Addressing the Issue of Digital Mapping of Soil Classes with Imbalanced Class Observations},
  author = {Sharififar, Amin and Sarmadian, Fereydoon and Malone, Brendan P. and Minasny, Budiman},
  date = {2019-09},
  journaltitle = {Geoderma},
  volume = {350},
  pages = {84--92},
  issn = {00167061},
  doi = {10.1016/j.geoderma.2019.05.016},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0016706118319499},
  urldate = {2019-06-26},
  abstract = {Considering the nature of soils distribution, an important modeling issue in soil class mapping is imbalanced class observations. Imbalanced number of data in observed soil classes in an area can result in the underestimation or loss of minority classes and an overestimation of the majority classes in predictive modeling. The effect of this phenomenon is that an area of land with comparatively fewer soil profile observations could be unmapped in the digital maps. To address this problem, this paper investigated the usefulness of data pretreatment techniques called over- and under-sampling of data applied on three predictive models including decision trees (DT), random forest (RF), and multinomial logistic regression (MNLR). The study area is situated in the northwest of Iran with 452 profiles observations on a regular grid covering about 12,000 ha. This area has 8 USDA soil great groups with an imbalanced frequency distribution. Results showed that modeling using imbalanced distribution of class observation caused uncertain maps with minority classes being lost and relatively poor accuracies. After data treatment, with over- and under-sampling, all models showed significant improvement in maintaining the minority classes, in both calibration and validation evaluations. Balancing the classes led to a notable decrease in uncertainty of all 3 models by decreasing the confusion index and raising the probability of occurrence for the soil classes in the final maps. Comparing the 3 models, decision trees showed the largest calibration and validation accuracies with and without data treatment. RF has an issue of overestimation of some of the majority classes. Data resampling technique can be a useful solution for dealing with imbalanced class observations to produce more certain digital soil maps.},
  file = {/Users/alazaro/Documents/Zotero/storage/KP4ATM9K/Sharififar_et_al-2019-Addressing_the_issue_of_digital_mapping_of_soil_classes_with_imbalanced_class.pdf},
  keywords = {_unread,training set},
  langid = {english}
}

@article{Sharififar2019MappingImbalancedSoil,
  title = {Mapping Imbalanced Soil Classes Using {{Markov}} Chain Random Fields Models Treated with Data Resampling Technique},
  author = {Sharififar, Amin and Sarmadian, Fereydoon and Minasny, Budiman},
  date = {2019-04},
  journaltitle = {Comput. Electron. Agric.},
  volume = {159},
  pages = {110--118},
  issn = {01681699},
  doi = {10.1016/j.compag.2019.03.006},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S016816991831737X},
  urldate = {2019-03-18},
  abstract = {Class imbalance is a problem in spatial predictive models; it occurs when classes with a large number of observations dominate the prediction and classes with much less number of observations are not predicted at all. It is a crucial topic as imbalanced distribution of soil classes occurs naturally. In this study, we address this problem using Markov chain random field modeling combined with a data resampling technique, i.e., random oversampling. The study area is about 12,000 ha with imbalanced distribution of eight soil great groups located in the northwest of Iran. Four algorithms were used to interpolate class observations, including fixed-path and random-path Markov chain random fields (FPth and RPth, respectively), indicator kriging simulation (IKS) and Markovian-type categorical simulation (MCS). All of the models provide prediction and simulation as outputs. The random oversampling technique was applied to the minority soil classes prior to modeling. Using original data that have imbalanced classes for mapping resulted in loss of the minority classes and relatively low Kappa agreement values for some models. Data oversampling increased overall accuracy and Kappa coefficient for FPth and RPth models. In addition, it led to the maintenance of one or two minority classes in the resulting maps. Overall, RPth model showed the highest overall accuracy and Kappa coefficient both in prediction (overall = 50\%, K = 0.33) and simulation (overall = 52\%, K = 0.36) outputs after data oversampling. Markov chain models combined with data oversampling technique can be used for mapping imbalanced soil classes.},
  file = {/Users/alazaro/Documents/Zotero/storage/NHKG37HZ/Sharififar_et_al-2019-Mapping_imbalanced_soil_classes_using_Markov_chain_random_fields_models_treated.pdf},
