@article{Flynn2019HighresolutionDigitalSoil,
  title = {High-Resolution Digital Soil Mapping of Multiple Soil Properties: An Alternative to the Traditional Field Survey?},
  shorttitle = {High-Resolution Digital Soil Mapping of Multiple Soil Properties},
  author = {Flynn, Trevan and de Clercq, Willem and Rozanov, Andrei and Clarke, Cathy},
  date = {2019-08-08},
  journaltitle = {South African Journal of Plant and Soil},
  volume = {36},
  pages = {237--247},
  issn = {0257-1862, 2167-034X},
  doi = {10.1080/02571862.2019.1570566},
  url = {https://www.tandfonline.com/doi/full/10.1080/02571862.2019.1570566},
  urldate = {2020-04-14},
  abstract = {Spatial information on soil particle size distribution and soil organic carbon (SOC) are important for land-use management, environmental models and policy-making. Digital soil mapping (DSM) techniques can quantitatively predict these soil properties using minimal resources. However, DSM has not been adequately evaluated at the farm-scale. The aim of this study was to optimise the DSM framework to produce farm-scale soil maps for 366 ha in the Sandspruit catchment, Western Cape, South Africa. Four feature selection techniques and eight predictive models were evaluated on their ability to predict particle size distribution and SOC. A boosted linear feature selection produced the highest accuracy for all but one soil property. The top-performing predictive models were robust linear models for gravel (ridge regression, RMSE 9.01\%, R{$\mkern1mu$}2 0.75), sand (support vector machine, RMSE 4.69\%, R{$\mkern1mu$}2 0.67), clay (quantile regression, RMSE 2.38\%, R{$\mkern1mu$}2 0.52) and SOC (ridge regression, RMSE 0.19\%, R{$\mkern1mu$}2 0.41). Random forest was the best predictive model for silt content (RMSE 4.12\%, R{$\mkern1mu$}2 0.53). This approach appears to be robust for farm-scale soil mapping where the number of observations is often small but high-resolution soil data are required.},
  file = {/Users/alazaro/Documents/Zotero/storage/KZ75623J/Flynn et al. - 2019 - High-resolution digital soil mapping of multiple s.pdf},
  langid = {english},
  number = {4},
  options = {useprefix=true}
}

@article{Flynn2019SemiautomaticDisaggregationNational,
  title = {Semi-Automatic Disaggregation of a National Resource Inventory into a Farm-Scale Soil Depth Class Map},
  author = {Flynn, Trevan and Rozanov, Andrei and de Clercq, Willem and Warr, Benjamin and Clarke, Cathy},
  date = {2019-03},
  journaltitle = {Geoderma},
  volume = {337},
  pages = {1136--1145},
  issn = {00167061},
  doi = {10.1016/j.geoderma.2018.11.003},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S001670611830702X},
  urldate = {2019-01-24},
  abstract = {Knowledge of soil depth spatial variability is important for land use management especially in dryland agriculture regions, which rely on climate and soils to provide adequate water and nutrients during the growing season. Soil spatial variability can be predicted from legacy soil data through machine learning techniques producing quantitative soil maps requiring minimal resources. South Africa has a country wide 1:250,000 scale resource map known as the Land Type Survey (LTS) which includes soil properties such as soil depth, soil class, root limiting layer, clay content, and texture. Each LTS polygon (land type), is comprised of unique soil – terrain patterns and is therefore, not a true soil map. This study aims to disaggregate the LTS into a farm-scale soil depth class map through a two-step disaggregation approach. First, landform elements were predicted through a pattern recognition algorithm known as geomorphons. Geomorphons, together with the original LTS were overlaid to produce polygons with unique distributions of soil. The polygons were disaggregated further to produce a raster map of soil depth classes through a soil map disaggregation algorithm known as DSMART. The first most probable class raster achieved an accuracy of 68\% and for the two most probable class rasters, an accuracy of 91\% was achieved. The two-step approach proved necessary for producing a farm-scale soil map. The result of this study is significant as it produced a soil depth class map from a national resource map at a scale and resolution (10 m) suitable for farm management.},
  annotation = {\textbf{Extracted Annotations (1/31/2019, 7:28:40 PM)}
\par
"The first most probable class raster achieved an accuracy of 68\% and for the two most probable class rasters, an accuracy of 91\% was achieved." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=1}{Flynn et al 2019:1136})
\par
"The two-step approach proved necessary for producing a farm-scale soil map. The result of this study is significant as it produced a soil depth class map from a national resource map at a scale and resolution (10 m) suitable for farm management." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=1}{Flynn et al 2019:1136})
\par
"Soil depth is one of the most important properties of soils and is one of the seven major considerations when evaluating soil quality (Bunning et al., 2011)." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=1}{Flynn et al 2019:1136})
\par
"Soil properties, including depth, may be highly variable on old land surfaces due to multiple cycles of erosion and deposition (Rozanov et al., 2017)." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=1}{Flynn et al 2019:1136})
\par
"Soil depth is difficult to estimate, and usually requires physical observations which are expensive. Alternatively, soil depth can be predicted through digital soil mapping (DSM) which requires fewer soil samples and, when soil legacy data is available, no soil samples at all." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=1}{Flynn et al 2019:1136})
\par
"South Africa has a 1:250,000 scale land resource map known as the LTS, which took over 30 years to complete and involved {$>$} 20 soil scientists (Land Type Staff, 1972-2002). Although not a true soil map, the LTS gives information to inform agricultural potential of South Africa by mapping unique distributions of climate, terrain, and soil. The soil information contained in the LTS includes local soil classes (soil series), soil depth ranges, root limiting layer, clay percent, and texture." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=1}{Flynn et al 2019:1136})
\par
"First, landform elements were predicted through a pattern recognition algorithm known as geomorphons. Geomorphons, together with the original LTS were overlaid to produce polygons with unique distributions of soil. The polygons were disaggregated further to produce a raster map of soil depth classes through a soil map disaggregation algorithm known as DSMART." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=1}{Flynn et al 2019:1136})
\par
"The LTS was developed in a hierarchical structure that is spatially scaled from the top down. At the top, are climatic regions which divide the country. Within each climatic region, areas are dissected by uniform patterns of landform elements (LFEs) and repeating soil patterns which are restricted to the individual polygons on the 1:250,000 map." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=1}{Flynn et al 2019:1136})
\par
"However, both LFEs and soil information were estimated from a recognisance scale soil survey and were not externally validated." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"There are many difficulties facing the disaggregation of the LTS, such as the coarse spatial extent, coarse feature scale, and the fact that the LTS does not have true soil polygons." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"The coarse spatial extent and feature scale of the LTS was necessary for nationwide coverage; however, this information is too coarse for land use management. To generatefiner-scale soil information," (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"LFEs must be classified to disaggregate the coarse land type polygons into basic LFEs that have associated soil attributes. In doing so, the LTS can be stratified into disaggregated polygons with more homogenous distribution of soil." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"There are many LFEs classification algorithms which include nested-clustering methods (Iwahashi and Pike, 2007), fuzzy landform classification (Irvin et al., 1997; Schmidt and Hewitt, 2004), object-based classification (Dragut, 2011), and more." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"However, these algorithms are notflexible to changes in scale, are often computationally inefficient, and/or do not coincide with slope position used by soil scientists in delineating the original LFEs (Jasiewicz and Stepinski, 2013; Libohova et al., 2016)." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"Developed by Jasiewicz and Stepinski (2013), geomorphons is a pattern recognition algorithm created to be computationally efficient, flexible to scale, and can correspond to slope position. Geomorphons classify the ten most common LFEs known asflat, peak, ridge, shoulder, spur, slope, hollow, foot-slope, valley, and pit." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"Other factors making the disaggregation of the LTS difficult is that there are very few georeferenced soil profiles and many land types do not have any georeferenced soil profiles." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"The lack of soil profiles makes updating the LTS problematic without an additional soil survey. Additionally, the LTS was developed by different soil scientists who surveyed different land types and the understanding of how to delineate the land types developed through its creation. Therefore, different soil environmental rules were established depending on who the surveyor was and the time of the survey." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"There are many approaches that have been developed that incorporate existing resource inventories into the DSM framework (Grunwald, 2006; Minasny and McBratney, 2016; Scull et al., 2003). These techniques include geostatistics, expert knowledge systems, and machine learning algorithms which have been successfully applied through kriging with external drift (Kempen et al., 2015), fuzzy logic (MacMillan et al., 2000; Silva et al., 2014; Smith et al., 2010; Yang et al., 2011), k-means clustering (Bui and Moran, 2001), decision trees (Nauman and Thompson, 2014; Sarmento et al., 2017; Subburayalu et al., 2014), and random forest (Häring et al., 2012; Nauman et al., 2014). An expert knowledge system through a fuzzy logic inference system known as SoLIM (Zhu, 1997), has been successfully applied to update the LTS into soil associations by van Zijl et al. (2013)." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"A technique known as "Disaggregating and Harmonisating of Soil Map Units Through Resampled Classification Trees" (DSMART) developed by Odgers et al. (2014) shows promise to disaggregate the LTS into a soil map." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"For each realisation, DSMARTfinds soil environmental relationships through the randomly assigned samples and covariates." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"The algorithm can be seen as a stochastic approach where multiple realisations give an estimate of the soil distribution. A benefit of the DSMART algorithm is that it can predict soil classes across soil polygon boundaries and predict all soil classes simultaneously." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"Odgers et al. (2014) who developed DSMART, showed DSMART's potential by disaggregating a soil legacy map in central Queensland, Australia. The authors achieved a 22.5\% accuracy for thefirst most probable class raster. However, the three most probable class rasters combined, classified 50\% of the soil classes correctly. Vincent et al. (2018) implemented DSMART to disaggregate a French resource map at a 1:250,000 scale. The authors achieved an accuracy of 41\% to 72\% depending on the validation technique used. Holmes et al. (2015) disaggregated a soilterrain polygon map through DSMART to predict Soil Groups of Western Australia. The authors achieved an accuracy of 41\% for the three most probable class rasters. Perhaps the most remarkable implementation of DSMART came from Chaney et al. (2016) who implemented DSMART with the Random Forest algorithm to disaggregate the Soil Survey Geographic database for the whole contiguous United States." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"These studies all focused on regional or even country scale and the algorithm has not yet been tested for farm-scale mapping (Malone et al., 2017)." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"This research serves as an investigative study to disaggregate the LTS into a farm-scale soil map at a resolution suitable for farming. Due to the importance of soil depth, this is the property selected for mapping." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"The process follows the hierarchical structure of the LTS byfirst stratifying the land types into homogenous distributions of soil, and then disaggregating these newly formed polygons through machine learning. The main idea behind this implementation was to adapt a large spatial scale DSM framework and adjust it for the local needs in South Africa. This can potentially increase the accessibility and usefulness of the LTS information to farmers by quantifying the spatial variability within the LTS." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=2}{Flynn et al 2019:1137})
\par
"Geomorphons and covariates were developed from a digital elevation model (DEM) using the Geographic Resource Analysis Support System (GRASS v7.2; GRASS Development Team, 2017) and the System for Automated Geoscientific Analyses (SAGA v2.3.2; Conrad et al., 2015), respectively. All models and statistical analysis were conducted in R software (R Core Team, 2017). Disaggregation was developed from the DSMART algorithm as in the rdsmart package (Odgers and Malone, 2017). However, the rdsmart code was adapted to incorporate any classification model available in the caret R package (Kuhn et al., 2018)." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=3}{Flynn et al 2019:1138})
\par
"The legends specify the LFEs found on each land type and the soil distributions on each LFE." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=3}{Flynn et al 2019:1138})
\par
"These LFEs are crest (1), scarp (2), mid-slope (3), foot-slope (4), and valley (5)." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=3}{Flynn et al 2019:1138})
\par
"To extract soil depth classes, the LTS depth ranges were averaged and divided into shallow (0 cm-40 cm), moderate (40 cm-80 cm) and deep classes ({$>$} 80 cm)." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=3}{Flynn et al 2019:1138})
\par
"A 30 m DEM" (\href{zotero://open-pdf/library/items/7VEUNSPP?page=3}{Flynn et al 2019:1138})
\par
\emph{¿Sería importante aplicar sinks filling? (\href{zotero://open-pdf/library/items/7VEUNSPP?page=4}{note on p.1139})}
\par
"The DEM was resampled to a 10 m resolution and had sinksfilled." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=4}{Flynn et al 2019:1139})
\par
"The concept of inspection density is that maps are predicted from point data and therefore, should have a similar sample density per area." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=4}{Flynn et al 2019:1139})
\par
"The disaggregation approach in this paper follows a two-step method to disaggregate the LTS shown in Fig. 3." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=4}{Flynn et al 2019:1139})
\par
"Thefinal geomorphon selected had a 200-cell search radius and aflatness threshold of 1°. It was thought that this geomorphon represented the rolling topography best and was most suitable to vectorise into polygons. To correspond with the LTS, the GM-10 was aggregated into a GM-5" (\href{zotero://open-pdf/library/items/7VEUNSPP?page=4}{Flynn et al 2019:1139})
\par
"To aggregate the geomorphon units, the geomorphon values were changed for each individual pixel of the original GM- 10." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=4}{Flynn et al 2019:1139})
\par
"The reclassification method used in this study is a simplification of the method developed by Libohova et al. (2016), who aggregated GM-10 into GM-5 based on a slope gradient threshold." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=4}{Flynn et al 2019:1139})
\par
"To determine if the geomorphon algorithm can mimic the "mental model" used to define LFEs in the LTS, an additional manually delineated LFEs was created (expert GM)." (\href{zotero://open-pdf/library/items/7VEUNSPP?page=4}{Flynn et al 2019:1139})
\par
\emph{Lo único que no me gusta es que no hacen un estudio comparativo entre las covariates.

Podrían explicar (aunque sea mínimamente) el porqué de la selección. (\href{zotero://open-pdf/library/items/7VEUNSPP?page=5}{note on p.1140})}
