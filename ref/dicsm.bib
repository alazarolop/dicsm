@article{Ugbaje2019FuzzyMeasurebasedMulticriteria,
  title = {Fuzzy Measure-Based Multicriteria Land Assessment for Rainfed Maize in {{West Africa}} for the Current and a Range of Plausible Future Climates},
  author = {Ugbaje, S.U. and Odeh, I.O.A. and Bishop, T.F.A.},
  date = {2019-03},
  journaltitle = {Comput. Electron. Agric.},
  volume = {158},
