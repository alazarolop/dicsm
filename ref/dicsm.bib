  shorttitle = {Integrating Geospatial and Multi-Depth Laboratory Spectral Data for Mapping Soil Classes in a Geologically Complex Area in Southeastern {{Brazil}}},
  author = {Vasques, G. M. and Demattê, J. A. M. and Viscarra Rossel, R. A. and Ramírez López, L. and Terra, F. S. and Rizzo, R. and De Souza Filho, C. R.},
