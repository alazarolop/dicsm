@collection{McBratney2012DigitalSoilAssessments,
  title = {Digital {{Soil Assessments}} and {{Beyond}}: {{Proceedings}} of the 5th {{Global Workshop}} on {{Digital Soil Mapping}} 2012, {{Sydney}}, {{Australia}}},
