@article{Minasny2008QuantitativeModelsPedogenesis,
  title = {Quantitative Models for Pedogenesis — {{A}} Review},
  author = {Minasny, Budiman and McBratney, Alex. B. and Salvador-Blanes, Sébastien},
  date = {2008-03},
  journaltitle = {Geoderma},
