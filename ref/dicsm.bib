  shorttitle = {Soil Mapping, Classification, and Pedologic Modeling},
  author = {Brevik, Eric C. and Calzolari, Costanza and Miller, Bradley A. and Pereira, Paulo and Kabala, Cezary and Baumgarten, Andreas and Jordán, Antonio},
  date = {2016-02},
  journaltitle = {Geoderma},
  volume = {264},
