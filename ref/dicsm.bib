  title = {A Multilingual Soil Profile Database ({{SDBm Plus}}) as an Essential Part of Land Resources Information Systems},
  author = {de la Rosa, D and Mayol, F and Moreno, F and Cabrera, F and Díaz-Pereira, E and Antoine, J},
