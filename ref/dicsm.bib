@article{Anders2011SegmentationOptimizationStratified,
  title = {Segmentation Optimization and Stratified Object-Based Analysis for Semi-Automated Geomorphological Mapping},
  author = {Anders, Niels S. and Seijmonsbergen, Arie C. and Bouten, Willem},
  date = {2011-12},
  journaltitle = {Remote Sens. Environ.},
