@article{Carre2007DigitalSoilAssessments,
  title = {Digital Soil Assessments: {{Beyond DSM}}},
  shorttitle = {Digital Soil Assessments},
  author = {Carré, F. and McBratney, Alex B. and Mayr, Thomas and Montanarella, Luca},
  date = {2007-11},
  journaltitle = {Geoderma},
  volume = {142},
  pages = {69--79},
  issn = {00167061},
  doi = {10.1016/j.geoderma.2007.08.015},
  url = {http://linkinghub.elsevier.com/retrieve/pii/S0016706107002261},
  urldate = {2018-03-05},
  annotation = {\textbf{Extracted Annotations (25/05/2018, 10:29:14)}
\par
"but rather as a technique for providing data and information for a new framework for soil assessment which we call Digital Soil Assessment (DSA)." (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=1}{Carré et al 2007:69})
\par
"Digital Soil Mapping (DSM), which has been defined as the creation and population of spatial soil information by the use of field and laboratory observational methods coupled with spatial and non-spatial soil inference systems" (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=1}{Carré et al 2007:69})
\par
"DSM can be used for two downstream applications which will be explained and defined in this paper: namely, Digital Soil Assessment (DSA) and Digital Soil Risk Assessment (DSRA)." (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=1}{Carré et al 2007:69})
\par
"DSA is the quantitative modelling of difficult-to-measure soil attributes, necessary for assessing threats to soil (erosion, decline of organic matter, compaction, salinisation, landslides, sealing, floods, decline of biodiversity) and soil functions (biomass production, environmental interactions, physical" (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=1}{Carré et al 2007:69})
\par
"support, production of raw material, cultural heritage, carbon pool, source of biodiversity) (European Commission, 2006b), using DSM outputs." (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=1}{Carré et al 2007:69})
\par
"DSRA is the quantitative evaluation of soil-related scenarios for providing policy guidance using the outputs from DSA plus socioeconomic data and more general information on the environment." (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=1}{Carré et al 2007:69})
\par
"DSM is therefore a precursor to DSA, and DSA is in turn a precursor to DSRA (Fig. 1)" (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=1}{Carré et al 2007:69})
\par
"DSM has many advantages such as costs, consistency and documentation, and can be easily updated when" (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=2}{Carré et al 2007:70})
\par
"new data become available. For present purposes, its biggest advantage is the capability of deriving uncertainties for predicted outcomes thus allowing the tracking of error propagation through the whole process." (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=2}{Carré et al 2007:70})
\par
"These are (1) the accuracy of the soil information, (2) the accuracy of the environmental covariates of soil, (3) the accuracy of the inference system, as well as the overall accuracy of DSM." (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=3}{Carré et al 2007:71})
\par
"comprises" (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=3}{Carré et al 2007:71})
\par
"One important principle of pedotransfer functions is that they should not be used unless their uncertainty can be evaluated" (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=3}{Carré et al 2007:71})
\par
"the evaluation of the error can be done by calculating the sum of the function error and the input data uncertainties" (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=3}{Carré et al 2007:71})
\par
"Moreover, the validity domains of each pedotransfer function should be evaluated to ensure that they are appropriate for the intended use." (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=3}{Carré et al 2007:71})
\par
"The seven most important functions evoked in the COM232 (the European Commission, 2006b) are: (1) biomass production, including in agriculture and forestry, (2) storing, filtering and transforming nutrients, substances and water, (3) biodiversity pool such as habitats, species and genes, (4) physical and cultural environment for humans and human activities, (5) source of raw materials, (6) acting as carbon pool, and (7) archive of geological and archeological heritage" (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=4}{Carré et al 2007:72})
\par
"Accuracy assessments can be done using Monte-Carlo simulation (Heuvelink, 1998)." (\href{zotero://open-pdf/library/items/4K7GRBZZ?page=8}{Carré et al 2007:76})},
  file = {/Users/alazaro/Documents/Zotero/storage/4K7GRBZZ/Carré_et_al-2007-Digital_soil_assessments.pdf},
  keywords = {_read,predictive soil mapping},
  langid = {english},
  number = {1-2}
}

@article{Carre2007EstimationPotentialImprovement,
  title = {Estimation and Potential Improvement of the Quality of Legacy Soil Samples for Digital Soil Mapping},
  author = {Carré, F. and McBratney, Alex B. and Minasny, B.},
  date = {2007-09},
  journaltitle = {Geoderma},
  volume = {141},
  pages = {1--14},
  issn = {00167061},
  doi = {10.1016/j.geoderma.2007.01.018},
  url = {http://linkinghub.elsevier.com/retrieve/pii/S0016706107000213},
  urldate = {2018-10-15},
  abstract = {Legacy soil data form an important resource for digital soil mapping and are essential for calibration of models for predicting soil properties from environmental variables. Such data arise from traditional soil survey. Methods of soil survey are generally empirical and based on the mental development of the surveyor, correlating soil with underlying geology, landforms, vegetation and air-photo interpretation. There are no statistical criteria for traditional soil sampling, and this may lead to biases in the areas being sampled. The challenge is to test the use of legacy data for largearea mapping (e.g. national or continental extents) in order to limit the funds of field survey for large-area mapping. The problem is then to assess the reliability and quality of the legacy soil databases that have been mainly populated by traditional soil survey, and if there is a possibility of additional funding for sampling, to determine where new sampling units should be located. This additional sampling can be used to improve and validate the prediction model.},
  file = {/Users/alazaro/Documents/Zotero/storage/3F9F7QLE/Carré et al. - 2007 - Estimation and potential improvement of the qualit.pdf},
  langid = {english},
  number = {1-2}
}

