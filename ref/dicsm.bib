}

@article{Angelini2017MultivariateMappingSoil,
  ids = {Angelini2017MultivariateMappingSoila},
  title = {Multivariate Mapping of Soil with Structural Equation Modelling: {{Multivariate}} Soil Mapping Using {{SEM}}},
