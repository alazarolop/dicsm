@article{Pedroso2010SegmentationAlgorithmDelineation,
  title = {A Segmentation Algorithm for the Delineation of Agricultural Management Zones},
  author = {Pedroso, Moacir and Taylor, James and Tisseyre, Bruno and Charnomordic, Brigitte and Guillaume, Serge},
  date = {2010-01},
  journaltitle = {Computers and Electronics in Agriculture},
