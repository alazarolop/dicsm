  title = {Retrieving Pedologist's Mental Model from Existing Soil Map and Comparing Data Mining Tools for Refining a Larger Area Map under Similar Environmental Conditions in {{Southeastern Brazil}}},
  author = {Silva, Sérgio Henrique Godinho and de Menezes, Michele Duarte and Owens, Phillip Ray and Curi, Nilton},
  date = {2016-04},
  journaltitle = {Geoderma},
  volume = {267},
