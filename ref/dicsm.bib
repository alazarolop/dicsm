@article{vanZijl2013DisaggregationLandTypes,
  title = {Disaggregation of Land Types Using Terrain Analysis, Expert Knowledge and {{GIS}} Methods},
  author = {van Zijl, George M and Le Roux, Pieter Al and Turner, David P},
