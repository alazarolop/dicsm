@article{Kerry2012DisaggregationLegacySoil,
  ids = {Kerry2012DisaggregationLegacySoila},
  title = {Disaggregation of Legacy Soil Data Using Area to Point Kriging for Mapping Soil Organic Carbon at the Regional Scale},
  author = {Kerry, Ruth and Goovaerts, Pierre and Rawlins, Barry G. and Marchant, Ben P.},
  date = {2012-01},
