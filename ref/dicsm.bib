@article{Lai2020IncrementaveragedKrigingComparison,
  title = {Increment-Averaged Kriging: A Comparison with Depth-Harmonized Mapping of Soil Exchangeable Sodium Percentage in a Cropping Region of Eastern {{Australia}}},
  shorttitle = {Increment-Averaged Kriging},
  author = {Lai, Y.R. and Orton, T.G. and Pringle, M.J. and Menzies, N.W. and Dang, Y.P.},
