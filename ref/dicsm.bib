@article{Nikiforova2019TheoreticalDevelopmentNatural,
  title = {Theoretical Development of a Natural Soil-Landscape Classification System: {{An}} Interdisciplinary Approach},
  shorttitle = {Theoretical Development of a Natural Soil-Landscape Classification System},
  author = {Nikiforova, Aleksandra A. and Bastian, Olaf and Fleis, Maria E. and Nyrtsov, Maxim V. and Khropov, Aleksandr G.},
