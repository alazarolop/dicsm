@article{Koch2013SoilSecuritySolving,
  title = {Soil {{Security}}: {{Solving}} the {{Global Soil Crisis}}},
  shorttitle = {Soil {{Security}}},
  author = {Koch, Andrea and McBratney, Alex and Adams, Mark and Field, Damien and Hill, Robert and Crawford, John and Minasny, Budiman and Lal, Rattan and Abbott, Lynette and O'Donnell, Anthony and Angers, Denis and Baldock, Jeffrey and Barbier, Edward and Binkley, Dan and Parton, William and Wall, Diana H. and Bird, Michael and Bouma, Johan and Chenu, Claire and Flora, Cornelia Butler and Goulding, Keith and Grunwald, Sabine and Hempel, Jon and Jastrow, Julie and Lehmann, Johannes and Lorenz, Klaus and Morgan, Cristine L. and Rice, Charles W. and Whitehead, David and Young, Iain and Zimmermann, Michael},
  date = {2013-11},
  journaltitle = {Glob. Policy},
  volume = {4},
