@article{Rasaei2020RescueRenewalLegacy,
  title = {Rescue and Renewal of Legacy Soil Resource Inventories in {{Iran}} as an Input to Digital Soil Mapping},
  author = {Rasaei, Zahra and Rossiter, David G. and Farshad, Abbas},
  date = {2020-06},
  journaltitle = {Geoderma Regional},
